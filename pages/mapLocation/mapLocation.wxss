/* mapLocation.wxss */
page {
  padding-top: 120rpx;
}

.searchView {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  z-index: 9999;
  background: #f5f5f5
}

.mapView map {
  width: 100%;
  height: 600rpx;
}

.addressCell {
  background-color: white;
  padding: 10rpx 20rpx 10rpx 20rpx;
  margin-bottom: 5rpx;
}

.address text:first-child {
  font-size: 30rpx;
  color: #e75858;
}

.address text:last-child {
  font-size: 30rpx;
  color: #000;
}

.detail text {
  font-size: 27rpx;
  color: #666;
}
